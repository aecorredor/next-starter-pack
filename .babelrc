{
  "env": {
    "production": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        ["emotion", { "hoist": true }],
        "transform-inline-environment-variables"
      ]
    },
    "development": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        ["emotion", { "sourceMap": true, "autoLabel": true }],
        "inline-dotenv"
      ]
    }
  }
}
